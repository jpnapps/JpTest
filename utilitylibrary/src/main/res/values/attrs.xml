<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>

        <attr name="cimv_width_percentage" format="integer"/>
        <attr name="cimv_tolerance_width" format="integer"/>
        <attr name="cimv_height_percentage" format="integer"/>
        <attr name="cimv_tolerance_height" format="integer"/>
        <attr name="cimv_blur_radius" format="integer"/>
        <attr name="cimv_aspect_ratio" format="float" />
        <attr name="cimv_parameter">
            <enum name="cimv_aspect_width" value="0"/>
            <enum name="cimv_aspect_height" value="1"/>
            <enum name="cimv_fixed_ratio" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ArcProgress">
        <attr name="arc_progress" format="integer"/>
        <attr name="arc_angle" format="float"/>
        <attr name="arc_stroke_width" format="dimension"/>
        <attr name="arc_max" format="integer"/>
        <attr name="arc_unfinished_color" format="color"/>
        <attr name="arc_finished_color" format="color"/>
        <attr name="arc_text_size" format="dimension"/>
        <attr name="arc_text_color" format="color"/>
        <attr name="arc_suffix_text" format="string"/>
        <attr name="arc_suffix_text_size" format="dimension"/>
        <attr name="arc_suffix_text_padding" format="dimension"/>
        <attr name="arc_bottom_text" format="string"/>
        <attr name="arc_bottom_text_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="Themes">
        <attr name="circleProgressStyle" format="reference"/>
        <attr name="donutProgressStyle" format="reference"/>
        <attr name="arcProgressStyle" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CircleProgress">
        <attr name="circle_progress" format="integer"/>
        <attr name="circle_max" format="integer"/>

        <attr name="circle_unfinished_color" format="color"/>
        <attr name="circle_finished_color" format="color"/>

        <attr name="circle_text_size" format="dimension"/>
        <attr name="circle_text_color" format="color"/>

        <attr name="circle_prefix_text" format="string"/>
        <attr name="circle_suffix_text" format="string"/>
    </declare-styleable>



    <declare-styleable name="RelativeTimeTextView">
        <attr name="relative_time_prefix" format="string" />
        <attr name="relative_time_suffix" format="string" />
        <attr name="reference_time" format="string" />
    </declare-styleable>
    <declare-styleable name="DeviceFitTextView">
      <attr name="percentage_width" format="integer"/>
      <attr name="percentage_height" format="integer"/>
        <attr name="fontName" format="string" />
        <attr name="isAuroResize" format="boolean" />
   </declare-styleable>


    <declare-styleable name="AspectRatioImageView">
        <attr name="aimv_width" format="integer"/>
        <attr name="aimv_height" format="integer"/>
        <attr name="aspectRatio" format="float" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="normal" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="DeviceFitImageView">
        <attr name="width_percentage" format="integer"/>
        <attr name="tolerance_width" format="integer"/>
        <attr name="height_percentage" format="integer"/>
        <attr name="tolerance_height" format="integer"/>
        <attr name="blur_radius" format="integer"/>
        <attr name="aspect_ratio" format="float" />
        <attr name="parameter">
            <enum name="aspect_width" value="0"/>
            <enum name="aspect_height" value="1"/>
            <enum name="fixed_ratio" value="2"/>
            <enum name="measure_width" value="3"/>
            <enum name="measure_height" value="4"/>
            <enum name="square" value="5"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="JpRelativeLayout">

        <attr name="jp_parameter">
            <enum name="aspect_width" value="0"/>
            <enum name="aspect_height" value="1"/>
            <enum name="fixed_ratio" value="2"/>
            <enum name="measure_width" value="3"/>
            <enum name="measure_height" value="4"/>
            <enum name="square" value="5"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="AnnotateRelativeLayout">
        <attr name="arl_width_percentage" format="integer"/>
        <attr name="arl_tolerance_width" format="integer"/>
        <attr name="arl_height_percentage" format="integer"/>
        <attr name="arl_tolerance_height" format="integer"/>
        <attr name="arl_parameter">
            <enum name="aspect_width" value="0"/>
            <enum name="aspect_height" value="1"/>
            <enum name="fixed_ratio" value="2"/>
            <enum name="measure_width" value="3"/>
            <enum name="measure_height" value="4"/>
            <enum name="square" value="5"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="WaveLoadingView">
        <attr name="wlv_borderWidth" format="dimension"/>
        <attr name="wlv_borderColor" format="color"/>
        <attr name="wlv_progressValue" format="integer"/>
        <attr name="wlv_shapeType" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="wlv_waveColor" format="color"/>
        <attr name="wlv_waveAmplitude" format="float"/>
        <attr name="wlv_titleTopSize" format="dimension"/>
        <attr name="wlv_titleCenterSize" format="dimension"/>
        <attr name="wlv_titleBottomSize" format="dimension"/>
        <attr name="wlv_titleTopColor" format="color"/>
        <attr name="wlv_titleCenterColor" format="color"/>
        <attr name="wlv_titleBottomColor" format="color"/>
        <attr name="wlv_titleTop" format="string"/>
        <attr name="wlv_titleCenter" format="string"/>
        <attr name="wlv_titleBottom" format="string"/>
    </declare-styleable>
